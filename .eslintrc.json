{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "prettier/prettier": "error",
      "import/no-unresolved": [2, { "caseSensitive": false }]
    }
  }